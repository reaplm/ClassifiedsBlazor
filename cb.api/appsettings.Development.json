{
    "Logging": {
        "Region": "us-east-1",
        "LogGroup": "AspNetCore.ClassifiedsBlazor",
        "IncludeLogLevel": true,
        "IncludeCategory": true,
        "IncludeNewline": true,
        "IncludeException": true,
        "IncludeEventId": false,
        "IncludeScopes": false,
        "LogLevel": {
            "Default": "Debug",
            "Sytem": "Information",
            "Microsoft": "Information"
        },
        "Console": {
            "LogLevel": {
                "Default": "Error",
                "System": "Information",
                "Microsoft": "Information"
            }
        },
        "Debug": {
            "LogLevel": {
                "Default": "Trace",
                "System": "Information",
                "Microsoft": "Information"
            }
        }
    },
    "BaseUrl": "https://localhost:5012/"
}
